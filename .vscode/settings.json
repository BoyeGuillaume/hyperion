{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/build": true,
        "**/target": true,
        "**/__pycache__": true,
        "**/_sys.abi3.so": true,
    },
    "editor.formatOnSave": true,
    "files.associations": {
        "time.h": "c",
        "stdint.h": "c"
    },
    "rust-analyzer.cargo.features": [
        "ext_all"
    ]
}